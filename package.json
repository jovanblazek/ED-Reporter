{
  "name": "ed-reporter",
  "version": "1.0.0",
  "description": "Discord bot for reporting Galnet news",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only --exit-child src/index.ts",
    "register-commands": "ts-node src/utils/registerCommands.ts",
    "migrate": "prisma migrate deploy",
    "build": "tsc",
    "start": "cd build && node src/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jovanblazek/ED-Reporter.git"
  },
  "author": "jovanblazek",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jovanblazek/ED-Reporter/issues"
  },
  "homepage": "https://github.com/jovanblazek/ED-Reporter#readme",
  "devDependencies": {
    "@sudolabs-io/eslint-config-sudolabs": "^1.5.66",
    "@types/node": "^18.7.6",
    "@types/node-cron": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "eslint": "^8.22.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "prisma": "^4.2.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@discordjs/rest": "^1.0.1",
    "@prisma/client": "^4.2.1",
    "deepl-node": "^1.5.0",
    "discord.js": "^14.2.0",
    "dotenv": "^16.0.1",
    "got": "^11.8.5",
    "node-cron": "^3.0.2",
    "winston": "^3.8.1"
  }
}
